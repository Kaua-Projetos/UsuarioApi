# Exemplo de variáveis de ambiente para Railway com Supabase
# Copie este arquivo, renomeie para .env e preencha os valores

# Configuração do banco de dados
DATABASE_URL=jdbc:postgresql://db.exemplo.supabase.co:5432/postgres
DATABASE_USERNAME=postgres
DATABASE_PASSWORD=sua_senha_secreta

# Configuração do Spring Boot
SPRING_PROFILES_ACTIVE=prod
PORT=8080

# Outras configurações
ALLOWED_ORIGINS=https://seu-frontend.app 